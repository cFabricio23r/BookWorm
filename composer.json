{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The skeleton application for the Laravel framework.",
    "keywords": ["laravel", "framework"],
    "license": "MIT",
    "require": {
        "php": "^8.2",
        "barryvdh/laravel-ide-helper": "^3.0",
        "guzzlehttp/guzzle": "^7.2",
        "juststeveking/http-status-code": "^3.0",
        "laravel/framework": "^11.0",
        "laravel/sanctum": "^4.0",
        "laravel/telescope": "^5.0",
        "laravel/tinker": "^2.8",
        "openai-php/laravel": "^0.9.0",
        "smalot/pdfparser": "^2.10",
        "spatie/laravel-data": "^4.6",
        "spatie/pdf-to-text": "^1.52"
    },
    "require-dev": {
        "fakerphp/faker": "^1.9.1",
        "larastan/larastan": "^2.9",
        "laravel/pint": "^1.0",
        "laravel/sail": "^1.18",
        "mockery/mockery": "^1.4.4",
        "nunomaduro/collision": "^8.1",
        "pestphp/pest": "^2.34",
        "pestphp/pest-plugin-laravel": "^2.4",
        "phpunit/phpunit": "^10.5",
        "spatie/laravel-ignition": "^2.0"
    },
    "autoload": {
        "files": [
            "supports/Helpers/helpers.php"
        ],
        "psr-4": {
            "App\\": "app/",
            "Supports\\": "supports/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "tests": [
            "./vendor/bin/pest"
        ],
        "feature-tests": [
            "./vendor/bin/pest --group=feature"
        ],
        "unit-tests": [
            "./vendor/bin/pest --group=unit"
        ],
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-update-cmd": [
            "@php artisan vendor:publish --tag=laravel-assets --ansi --force"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ],
        "pint": [
            "./vendor/bin/pint"
        ],
        "phpstan": [
            "./vendor/bin/phpstan analyse --memory-limit=-1"
        ],
        "update-ide-helpers": [
            "@php artisan ide-helper:generate",
            "@php artisan ide-helper:models --write-mixin",
            "@php artisan ide-helper:meta"
        ],
        "test-ci": [
            "@php artisan test --stop-on-failure --compact --profile --order-by=defects"
        ],
        "test-local": [
            "@php -d memory_limit=-1 artisan test --stop-on-failure --compact --profile --parallel --order-by=defects"
        ],
        "test-ci-unit": [
            "@php artisan test --stop-on-failure --compact --profile --order-by=defects --group=unit --parallel"
        ],
        "test-ci-feature": [
            "@php -d memory_limit=-1 artisan test --stop-on-failure --compact --profile --order-by=defects --group=feature --parallel"
        ],
        "verify-local-dev": [
            "@phpstan",
            "@pint",
            "@test-local",
            "@update-ide-helpers"
        ]
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "php-http/discovery": true
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
